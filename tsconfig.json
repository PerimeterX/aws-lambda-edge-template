{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "outDir": "lib",
    "resolveJsonModule": true,
  },
  "include": [
    "basic_example/src"
  ],
  "exclude": ["lib"]
}